{
  "name": "@ccms/api",
  "description": "Tools allowing to connect with the project API",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup --watch & tsc --declaration --declarationMap --emitDeclarationOnly --watch",
    "build": "rm -rf dist && tsup & tsc --declaration --declarationMap --emitDeclarationOnly",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "clean": "rm -rf .turbo & rm -rf node_modules & rm -rf dist",
    "generate": "node scripts/generate.js"
  },
  "dependencies": {
    "@ccms/typings": "workspace:*",
    "tsup": "5.11.11"
  },
  "devDependencies": {
    "@ccms/node": "workspace:*",
    "cross-fetch": "3.1.5",
    "fetch-cookie": "1.0.0",
    "jest": "27.4.7",
    "neverthrow": "4.3.1",
    "openapi-typescript": "5.1.1",
    "openapi-typescript-fetch": "1.1.2",
    "ts-jest": "27.1.3"
  }
}
