{
  "name": "@ccms/website",
  "description": "Static website created with Next.js",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port $WEBSITE_PORT",
    "build": "next build",
    "start": "next start --port $WEBSITE_PORT",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "clean": "rm -rf node_modules & rm -rf .next"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@ccms/node": "workspace:*",
    "@next/eslint-plugin-next": "12.1.0",
    "@types/react": "17.0.39"
  }
}
