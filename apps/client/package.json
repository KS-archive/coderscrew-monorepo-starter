  "dependencies": {
    "@ccms/ui": "*",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-location": "3.3.3",
    "react-location-devtools": "3.3.3"
  },
  "devDependencies": {
    "@ccms/node": "*",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react": "1.1.4",
    "vite": "2.7.13",
    "vite-tsconfig-paths": "3.3.17"
  }
}
