{
  "name": "@ccms/docs",
  "description": "Project documentation created using Docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "docusaurus start --port $DOCUMENTATION_PORT --no-open",
    "build": "docusaurus build --out-dir dist",
    "start": "docusaurus serve --port $DOCUMENTATION_PORT",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "clean": "docusaurus clear && rm -rf node_modules"
  },
  "dependencies": {
    "@algolia/client-search": "4.12.1",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@ccms/node": "workspace:*",
    "@types/react": "17.0.39",
    "webpack": "5.69.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
